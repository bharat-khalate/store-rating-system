{
  // Visit https://aka.ms/tsconfig to read more about this file
  "compilerOptions": {
    // File Layout
    "rootDir": "./src",
    "outDir": "./dist",


    "module": "es2020",
    "target": "es2020",
    "moduleResolution": "node", 

    "strict": true,
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true, 
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
